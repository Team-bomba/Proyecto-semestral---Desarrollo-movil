<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectosemestral">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presenters.HomeActivity"></activity>
        <activity android:name=".presenters.tendenciasActivity" />
        <activity android:name=".presenters.PostsActivity"></activity>
        <activity android:name=".presenters.PostViewActivity" />
        <activity android:name=".presenters.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presenters.AnimesActivity" />
        <activity android:name=".presenters.PerfilViewActivity" />
    </application>

</manifest>